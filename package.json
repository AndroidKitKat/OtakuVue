{
  "name": "boilerplate2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "watch": "npm run serve",
    "start": "vue-cli-service serve --fix --open",
    "service": "vue-cli-service",
    "build": "npm run service build",
    "build:pre": "npm run service build --preprocess",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint": "npm run service lint",
    "test": "npm run test:unit npm run test:e2e",
    "test:unit": "npm run service test:unit",
    "test:unit:watch": "npm run service npm run test:unit --watchAll --notify --notifyMode change",
    "test:e2e": "npm run service test:e2e --headless"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.0",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-e2e-cypress": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.4",
    "@vue/cli-plugin-unit-jest": "^3.1.1",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "6.26.3",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "html-webpack-plugin": "^5.5.3",
    "node-sass": "^4.10.0",
    "prerender-spa-plugin": "^3.4.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.8.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.4.0",
    "vue-template-compiler": "^2.5.17",
    "vuepress": "^1.9.9"
  }
}
